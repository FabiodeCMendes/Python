#video- https://www.youtube.com/watch?v=IvMdA471dPw

# origens : anotações.txt e vendas.xlsx
app.py

from openpyxl import load_workbook, Workbook

# LER DADOS DE QUALQUER PLANILHA
plan_vendas = load_workbook('c:/vendas.xlsx')
pag_vendas = plan_vendas['sheet']

for linha in plan1.iter_rows( values_only = true):
     print(linha)	


# AUTOMATIZAR ENTRADA DE DADOS EM PLANILHA (importar do txt para planilha)
plan_contas = Workbook()
pag1 = planilha contas.active

#Abrir arquivo txt
with open('anotações.txt','r',encoding = 'utf-8') as :arquivo
       for linha in arquivo
            pag1.append(linha.split(','))
plan_contas.save('contas_pg.xlsx')

